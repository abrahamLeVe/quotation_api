{
  "kind": "collectionType",
  "collectionName": "product_ratings",
  "info": {
    "singularName": "product-rating",
    "pluralName": "product-ratings",
    "displayName": "Product Rating"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "rating": {
      "type": "integer",
      "required": true,
      "default": 0,
      "max": 5,
      "min": 0
    },
    "producto": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::product.product"
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "message": {
      "type": "text",
      "maxLength": 300,
      "minLength": 3,
      "required": false
    },
    "state": {
      "type": "boolean",
      "default": false,
      "required": true
    }
  }
}
