{
  "kind": "collectionType",
  "collectionName": "contacts",
  "info": {
    "singularName": "contact",
    "pluralName": "contacts",
    "displayName": "contact",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "contact_type": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::contact-type.contact-type",
      "inversedBy": "contacts"
    },
    "name": {
      "type": "string",
      "required": true,
      "minLength": 3,
      "maxLength": 90
    },
    "email": {
      "type": "email",
      "required": true,
      "minLength": 5,
      "maxLength": 90
    },
    "phone": {
      "type": "string",
      "required": true,
      "maxLength": 12,
      "minLength": 6
    },
    "message": {
      "type": "text",
      "required": true,
      "unique": true,
      "maxLength": 300,
      "minLength": 3
    },
    "responseContact": {
      "type": "richtext",
      "required": false
    },
    "stateMessage": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "title": {
      "type": "string",
      "required": true,
      "maxLength": 70,
      "minLength": 3
    },
    "rating": {
      "type": "integer",
      "required": true,
      "min": 0,
      "max": 5,
      "default": 0
    }
  }
}
